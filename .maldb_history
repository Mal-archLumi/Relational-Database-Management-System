import sys
sys.path.insert(0, '.')
from src.core.database import Database
db = Database('test.maldb')
# Simple CREATE TABLE
try:
    result = db.execute('CREATE TABLE users (id INT, name VARCHAR(255), age INT)')
    print('✅ Simple CREATE TABLE works')
except Exception as e:
    print(f'❌ Error: {e}')
# Complex CREATE TABLE
try:
    result = db.execute('CREATE TABLE employees (id INT PRIMARY KEY, emp_code VARCHAR(20) UNIQUE, name VARCHAR(100) NOT NULL, ssn TEXT ENCRYPTED)')
    print('✅ Complex CREATE TABLE with constraints works')
except Exception as e:
    print(f'❌ Error: {e}')
db.close()
